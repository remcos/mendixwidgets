<?xml version="1.0" encoding="utf-8" ?>
<widget id="ShowByCondition.widget.ShowByCondition" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>ShowByCondition</name>
    <description>This widget will hide or show its parent, based on the result of a Microflow.</description>
    <icon></icon>

    <properties>
      <property key="microflowName" type="microflow" required="false">
  			<caption>Boolean Microflow</caption>
  			<category>Data source</category>
  			<description>Select the Microflow that determines whether the element should be shown.</description>
  			<returnType type="Boolean" />
  		</property>
          <property key="nanoflow" type="nanoflow" required="false">
           <caption>Boolean nanoflow</caption>
           <category>Data source</category>
           <description>(Ignored if a microflow is set)</description>
           <returnType type="Boolean" />
          </property>
          <property key="hiddenClass" type="string" required="false" defaultValue="hidden">
            <caption>Hidden Class</caption>
            <category>Behaviour</category>
            <description>if hiding elements by class, this is the class that will be added. You may need to define style rules for this class in your SASS/CSS/</description>
          </property>
		<property key="elementClassFalse" type="string" required="false">
          <caption>Element to hide on false</caption>
          <category>Behaviour</category>
          <description>When the MF returns false, elements with this class will be hidden.</description>
      </property>
	  <property key="elementClassTrue" type="string" required="false">
          <caption>Element to hide on true</caption>
          <category>Behaviour</category>
          <description>When the MF returns true, elements with this class will be hidden.</description>
      </property>
      <property key="returnValue" type="boolean" defaultValue="true">
          <caption>Show parent on true</caption>
          <category>Behaviour</category>
          <description>'Yes' shows the parent element on a true outcome, 'No' on a false outcome. Ignored when class to hide is filled in. </description>
      </property>
	  
    </properties>
</widget>
